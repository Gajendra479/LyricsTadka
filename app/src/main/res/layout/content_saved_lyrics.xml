<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/lin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.vianet.lyricstadka.Activityclass.SavedLyrics"
    tools:showIn="@layout/activity_saved_lyrics">

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:id="@+id/saved_recycle_view"
        android:layout_height="match_parent">
    </android.support.v7.widget.RecyclerView>


    <FrameLayout
        android:layout_width="match_parent"
        android:id="@+id/sacvedContainer"
        android:layout_height="match_parent">


    </FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:id="@+id/saved_data_not_available"
        android:text="@string/data_notfound"
        android:layout_gravity="center"
        android:layout_height="wrap_content" />

</FrameLayout>
